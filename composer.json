{
    "name": "paymaxi/fractal-bundle",
    "license": "MIT",
    "type": "library",
    "description": "A Symfony2 Bundle for Fractal by League. Implements dependency injection into your transformers",
    "autoload": {
        "psr-4": {
            "Paymaxi\\FractalBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "league/fractal": "^0.16",
        "paymaxi/compiler": "^0.1",
        "symfony/framework-bundle": "^2.7|^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
